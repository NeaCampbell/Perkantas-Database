{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HeaderToken": "password",
  "HashFormat": "X3",
  "HashCultureInfo": "en-us",
  "DatabaseConfig": {
    "DbServiceType": "MySql",
    "DatabasePath": "config/database.config",
    "QueryPath": "config/querylist.config"
  },
  "ThreadConfig": {
    "MaxQueueCount": 500,
    "ThreadCount": 16,
    "ThreadPoolWaitingTime": 100,
    "RequestWaitingTime": 10,
    "MaxRequestTimeout": 10000
  }
}
